= render :partial => "shared/argylenavbar", :user => @user

.row
  .col-md-12
    %p Edit yer post!
    = form_for @post, :url=>{:action=>'update'} do |post_form|
      %p
        = post_form.text_field :title
      %p
        = post_form.text_area :body, size: "30x10"
      %p
        = post_form.submit "Submit!"